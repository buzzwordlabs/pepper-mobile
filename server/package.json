    "start": "node server.js",
    "start:w": "dotenv -e ../.env nodemon server.js",
    "start:tsc:w": "yarn cleanjs && run-p tsc:w start:w",
    "tsc:w": "tsc -w",
    "tsc": "tsc",
    "script:metrics": "dotenv -e ../.env -- ts-node scripts/metrics.ts --metrics",
    "db:migration:run": "yarn cleanjs && tsc && dotenv -e ../.env typeorm migration:run",
    "db:migration:create": "yarn cleanjs && tsc && dotenv -e ../.env -- typeorm migration:create",
    "db:migration:generate": "yarn cleanjs && tsc && dotenv -e ../.env -- typeorm migration:generate",
    "db:migration:revert": "dotenv -e ../.env typeorm migration:revert",
    "db:migration:run:prod": "tsc && typeorm migration:run",
    "cleanjs": "find . -not -path \"./node_modules/*\" -name \"*.js\" -type f -delete"
  },
